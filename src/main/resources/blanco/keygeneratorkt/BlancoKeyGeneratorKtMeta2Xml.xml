        </tablecolumn>
        <tablecolumn name="type">
          <value>型</value>
        </tablecolumn>
        <tablecolumn name="generic">
          <value>総称型</value>
        </tablecolumn>
        <tablecolumn name="description">
          <value>説明</value>
        </tablecolumn>
      </tableblock>

      <tableblock name="blancokeygenerator-itemlist" waitY="2" titleheight="3" rowname="field">
        <startstring>キーバリューストアテーブル定義・項目一覧</startstring>

        <tablecolumn name="no">
          <value>No.</value>
        </tablecolumn>
        <tablecolumn name="name">
          <value>フィールド名</value>
        </tablecolumn>
        <tablecolumn name="type">
          <value>型</value>
        </tablecolumn>
        <tablecolumn name="generic">
          <value>総称型</value>
        </tablecolumn>
        <tablecolumn name="annotation">
          <value>アノテーション</value>
        </tablecolumn>
        <tablecolumn name="default">
          <value>デフォルト</value>
        </tablecolumn>
        <tablecolumn name="defaultKt">
          <value>デフォルト(Kt)</value>
        </tablecolumn>
        <tablecolumn name="keyitem">
          <value>キー項目</value>
        </tablecolumn>
        <tablecolumn name="notNullable">
          <value>非Nullable</value>
        </tablecolumn>
        <tablecolumn name="constructorArg">
          <value>コンストラクタ引数</value>
        </tablecolumn>
        <tablecolumn name="maxLength">
          <value>最大桁数</value>
        </tablecolumn>
        <tablecolumn name="description">
          <value>説明</value>
        </tablecolumn>

        <valuemapping>
          <result>true</result>
          <source>○</source>
        </valuemapping>

        <valuemapping>
          <result>not</result>
          <source>×</source>
        </valuemapping>

      </tableblock>

      <tableblock name="blancokeygenerator-keylist" waitY="2" titleheight="3" rowname="key">
        <startstring>キーバリューストアテーブル定義・キー一覧</startstring>

        <tablecolumn name="no">
          <value>No.</value>
        </tablecolumn>
        <tablecolumn name="name">
          <value>キー名</value>
        </tablecolumn>
        <tablecolumn name="keyPart">
          <value>検索部</value>
        </tablecolumn>
        <tablecolumn name="valuePart">
          <value>非検索部</value>
        </tablecolumn>
        <tablecolumn name="description">
          <value>説明</value>
        </tablecolumn>

        <valuemapping>
          <result>true</result>
          <source>○</source>
        </valuemapping>

        <valuemapping>
          <result>not</result>
          <source>×</source>
        </valuemapping>

      </tableblock>

    </blancocalcparser>
