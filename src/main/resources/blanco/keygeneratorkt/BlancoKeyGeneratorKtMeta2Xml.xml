<?xml version="1.0" encoding="UTF-8"?>
<!-- blancoCalcParser define file for blancoKeyGenerator -->
<blanco version="0.1.0">
  <target name="blancoCalcParser">
    <blancocalcparser name="keygenerator">
      <propertyblock name="blancokeygenerator-bucketcommon" waitY="1">
        <startstring>キーバリューストアバケット定義・共通</startstring>
        <propertykey name="name" waitX="2">
          <value>バケット定義書ID</value>
        </propertykey>
        <propertykey name="package" waitX="2">
          <value>パッケージ</value>
        </propertykey>
        <propertykey name="description" waitX="2">
          <value>説明</value>
        </propertykey>
        <propertykey name="fileDescription" waitX="2">
          <value>ファイル説明</value>
        </propertykey>
      </propertyblock>


      <tableblock name="blancokeygenerator-bucketlist" waitY="2" titleheight="3" rowname="bucket">
      <startstring>キーバリューストアバケット定義・一覧</startstring>
        <tablecolumn name="no">
          <value>No.</value>
          <tablecolumn name="name">
            <value>バケットID</value>
          </tablecolumn>
        </tablecolumn>
        <tablecolumn name="kvsType">
          <value>KVSタイプ</value>
        </tablecolumn>
        <tablecolumn name="tableIdLength">
          <value>table識別子桁数</value>
        </tablecolumn>
        <tablecolumn name="version">
          <value>バージョン</value>
        </tablecolumn>
        <tablecolumn name="description">
          <value>説明</value>
        </tablecolumn>
      </tableblock>

      <propertyblock name="blancokeygenerator-common" waitY="1">
        <startstring>キーバリューストアテーブル定義・共通</startstring>
        <propertykey name="displayName" waitX="2">
          <value>テーブル表示名</value>
        </propertykey>
        <propertykey name="bucket" waitX="2">
          <value>バケットID</value>
        </propertykey>
        <propertykey name="tableNumber" waitX="2">
          <value>テーブル番号</value>
        </propertykey>
        <propertykey name="name" waitX="2">
          <value>クラス名</value>
        </propertykey>
        <!--
        <propertykey name="namespace" waitX="2">
          <value>名前空間</value>
        </propertykey>
        -->
        <propertykey name="package" waitX="2">
          <value>パッケージ</value>
        </propertykey>
        <propertykey name="generic" waitX="2">
          <value>総称型</value>
        </propertykey>
        <propertykey name="annotation" waitX="2">
          <value>アノテーション</value>
        </propertykey>
        <propertykey name="annotationKt" waitX="2">
          <value>アノテーション(Kt)</value>
        </propertykey>
        <propertykey name="description" waitX="2">
          <value>説明</value>
        </propertykey>
        <propertykey name="fileDescription" waitX="2">
          <value>ファイル説明</value>
        </propertykey>
        <propertykey name="keyIdLength" waitX="2">
          <value>キー識別子桁数</value>
        </propertykey>
        <propertykey name="maxKeyLength" waitX="2">
          <value>キー最大桁数</value>
        </propertykey>
        <propertykey name="recordSequenceLength" waitX="2">
          <value>レコード分割番号桁数</value>
        </propertykey>
        <propertykey name="tableVersion" waitX="2">
          <value>バージョン</value>
        </propertykey>

        <valuemapping>
          <result>true</result>
          <source>○</source>
        </valuemapping>
      </propertyblock>

      <propertyblock name="blancokeygenerator-extends" waitY="1">
        <startstring>キーバリューストアテーブル定義・継承</startstring>
        <propertykey name="name" waitX="2">
          <value>クラス名</value>
        </propertykey>
        <propertykey name="package" waitX="2">
          <value>パッケージ</value>
        </propertykey>
        <propertykey name="generic" waitX="2">
          <value>総称型</value>
        </propertykey>
      </propertyblock>

      <tableblock name="blancokeygenerator-implements" waitY="2" titleheight="2" rowname="import">
        <startstring>キーバリューストアテーブル定義・実装</startstring>

        <tablecolumn name="no">
          <value>No.</value>
        </tablecolumn>
        <tablecolumn name="name">
          <value>インタフェース名</value>
        </tablecolumn>

      </tableblock>

      <tableblock name="blancokeygenerator-import" waitY="2" titleheight="2" rowname="import">
